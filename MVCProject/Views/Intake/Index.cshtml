@{
    Layout = "~/Views/Shared/AdminDashBoard.cshtml";
}
@model  List<Intake>
<div>
    @{
                        <table class="table table-striped">
            <thead>
                  <th>id</th>
                  <th>name</th>        

            </thead>
    <tbody>
    @foreach(var item in Model)
        {
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Name
                </td>
            </tr>

        }
            </tbody>

                  </table>
    }
</div>
<div>

    <input type="button" value="Add Intake" class="btn btn-info" asp-action="add" asp-asp-controller="intake"/>
    <input type="button" value="Remove Intake" class="btn btn-danger"/>

</div>

