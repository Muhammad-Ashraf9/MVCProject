@{
    Layout = "~/Views/Shared/AdminDashBoard.cshtml";
}
@model  List<Intake>
<div>
    @{
                        <table class="table table-striped col-8">
            <thead>
                   <th>#</th>
                  <th>id</th>
                  <th>name</th> 
                  <th>program</th>
            </thead>
    <tbody>
    @foreach(var item in Model)
        {
            <tr>
            <td class="col-1">
                <input type="checkbox"/>
            </td>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Name
                </td>
                <td>
                @item.Program.Name
                </td>
                <td>
                <a asp-action="Delete" asp-controller="Intake" asp-route-id = "@item.Id" class="btn btn-danger" id="Deletebtn">Delete</a>
                </td>
                 <td>
                 <a asp-action="Edit" asp-controller="Intake" asp-route-id = "@item.Id" class="btn btn-warning">Edit</a>
                 </td>
                      <td>
                          <a asp-action="ViewTracks" asp-controller="Intake" asp-route-id = "@item.ProgramId" asp-route-intake="@item.Id" class="btn btn-info">Edit Tracks</a>
                     </td>
                </tr>

        }
            </tbody>

                  </table>
    }
</div>
<div>

    <a  class="btn btn-info" asp-action="add" asp-asp-controller="intake"> Add Intake </a> 

</div>

